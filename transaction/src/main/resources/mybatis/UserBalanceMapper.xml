<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zc.transaction.repository.UserBalanceRepository">

    <insert id="insert" parameterType="com.zc.transaction.entity.UserBalance">
        INSERT INTO `tuser_balance` (`id`, `name`, `balance`)
        VALUES (#{id}, #{name}, #{balance});
    </insert>

</mapper>