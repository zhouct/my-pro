<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zc.transaction.repository.UserRepository">

    <insert id="addUser" parameterType="com.zc.transaction.entity.User">
        INSERT INTO `tuser`(`name`)
        VALUES (
            #{name}
        )
    </insert>

</mapper>